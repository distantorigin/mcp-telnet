  },
  "bugs": {
    "url": "https://github.com/distantorigin/mcp-telnet/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.13.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.6.3",
    "shx": "^0.3.4"
  }
}
